{
  "name": "payment-provider-example",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx},!node_modules/*": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,graphql,gql},!node_modules/*": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.6",
    "@vtex/payment-provider": "^1.4.0",
    "axios": "^0.24.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@vtex/api": "^6.45.6",
    "@vtex/prettier-config": "^0.1.3",
    "eslint": "^6.8.0",
    "eslint-config-vtex": "^12.2.1",
    "husky": "^4.2.0",
    "lint-staged": "^10.0.2",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5"
  }
}
